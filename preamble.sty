% doublecell in tikzcd
\newcommand*{\doublecell}[2][rd]{%
    \ar[
        #1,
        phantom, "#2"{description, inner sep=0mm},
        start anchor={[xshift=0ex, yshift=0ex]center},
        end anchor={[xshift=0ex, yshift=0ex]center},
        ]
}
%pullbacl in tikzcd
\newcommand*{\pullbackA}[1][rd]{%
    \doublecell[#1, "\mbox{\rotatebox[origin=c]{45}{$\lrcorner$}}"{marking, near start}]{}
}
\newcommand*{\pullback}[1][rd]{%
    \doublecell[#1, "\lrcorner"{description, near start, inner sep=0mm}]{}
}
% sar in tikzcd
\newcommand*{\sar}[1][]{%
